	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "dev_dist",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "yarn",
			"runtimeArgs": [
				"run",
				"dev"
			],
			"args": [
				"./dist"
			],
			"sourceMaps": true,
			"protocol": "inspector",
			"console": "internalConsole"
		}
	]
}